implementation
uses System;

function PrintNth(xs: array[2] of String; n: Integer);
begin
    var first := xs[n];
    WriteLn(first);
end;

initialization
    var x := [
        StringConcat('Hello, ', 'World'), 
        StringConcat('Hello, ', 'Everyone')
    ];

    var y := x;
    
    PrintNth(y, 0);
    PrintNth(y, 1);
end
